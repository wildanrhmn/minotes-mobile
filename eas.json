{
    "cli": {
      "version": ">= 8.0.0"
    },
    "build": {
      "development": {
        "developmentClient": true,
        "distribution": "internal",
        "channel": "development"
      },
      "preview": {
        "distribution": "internal",
        "android": {
          "buildType": "apk",
          "image": "latest"
        },
        "env": {
          "EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID": "YOUR_GOOGLE_WEB_CLIENT_ID",
          "FBSDK_APP_ID": "YOUR_FBSDK_APP_ID",
          "FBSDK_CLIENT_TOKEN": "YOUR_FBSDK_CLIENT_TOKEN",
          "GOOGLE_SERVICES_JSON": "YOUR_GOOGLE_SERVICES_JSON"
        },
        "channel": "preview"
      },
      "production": {
        "channel": "production"
      }
    },
    "submit": {
      "production": {}
    }
  }